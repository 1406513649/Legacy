@article{Brannon:2007,
	Abstract = {Validating simulated predictions of internal damage within armor ceramics is preferable to simply assessing a model's ability to predict penetration depth, especially if one hopes to perform subsequent ``second strike'' analyses. We present results of a study in which crack networks are seeded by using a statistically perturbed strength whose median is inherited from a deterministic ``smeared damage'' model with adjustments to reflect experimentally established size effects. This minor alteration of an otherwise conventional damage model noticeably mitigates mesh dependencies and, at virtually no computational cost, produces far more realistic cracking patterns that are well suited for validation against X-ray computed tomography (XCT) images of internal damage patterns. For Brazilian, spall, and indentation tests, simulations share qualitative features with externally visible damage. However, the need for more stringent quantitative validation, software quality testing, and sub-surface XCT validation, is emphasized.},
	Annote = {Paper deals with the need to further verify and validate damage models.  To the authors knowledge, no damage model has been verified and shown to give reasonable results using a single set of input parameters for multiple loadings.  This paper highlights this issue as well as explain (in little detail) using statistically perturbed values of strength parameters in each finite element.},
	Author = {Brannon, Rebecca M. and Wells, Joseph M. and Strack, O. Erik},
	Date-Modified = {2009-05-05 15:25:44 -0600},
	Journal = METALL_MATER_TRANS_A,
	Keywords = {simulation, verification, validation, brittle damage, diagnostics, armor ceramics, cracking, impact},
	Read = {Yes},
	Title = {Validating Theories for Brittle Damage},
	Volume = {A},
	Year = {2007},
	Bdsk-File-1 = {}}

@article{Houlsby:2005,
	Abstract = {The elastic behaviour of granular materials is non-linear, in that the small-strain tangent stiffness depends on the stress level. The elastic moduli typically vary as power functions of the mean stress. Simple models of this non-linearity can result in behaviour that violates the laws of  thermodynamics. To guarantee that an elasticity model is thermodynamically acceptable it must be possible to derive the elastic behaviour from a free energy potential (or alternatively from a complementary energy potential). In this paper elasticity models are derived that allow for variation of elastic moduli as power functions of mean stress, while guaranteeing thermodynamic acceptability.  The important issue of the dependence of secant stiffness on strain amplitude (a phenomenon related to dissipation processes in the soil) is acknowledged but not addressed here.
},
	Annote = {this paper shows that a hyperelastic formulation can lead to an anisotropic-nonlinear tangent modulus that is stress dependent.


},
	Author = {Houlsby, G. T. and Amorosi, A. and Rojas, E.},
	Date-Modified = {2010-01-27 19:27:06 -0700},
	Journal = GEOTECHNIQUE,
	Keywords = {induced anisotropy, anisotropy, elasticity, stiffness, stress, pressure, elastic moduli},
	Pages = {383--392},
	Title = {Elastic moduli of soils dependent on pressure: a hyperelastic formulation},
	Volume = {55},
	Year = {2005},
	Bdsk-File-1 = {}}

